.placeSwitchGroup {
  display: flex;
  align-items: center;
  margin-bottom: 3rem;
}

.searchElem {
  position: relative;
  width: 50rem;
  height: 6rem;

  & svg {
    position: absolute;
    top: 1.5rem;
    right: 2rem;
  }
}

.searchBtn {
  width: 50rem;
  height: 6rem;
  border: none;
  background-color: #f5f5f5;
  border-radius: 1rem;
  box-sizing: border-box;
  padding: 1.5rem 2rem;
}
